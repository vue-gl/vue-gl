{
  "name": "vue-gl",
  "version": "0.22.1",
  "description": "Vue.js components rendering 3D graphics reactively via three.js",
  "keywords": [
    "Vue",
    "Vuejs",
    "WebGL",
    "three",
    "threejs",
    "3D",
    "Graphics",
    "Canvas"
  ],
  "homepage": "https://vue-gl.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-gl/vue-gl.git"
  },
  "bugs": {
    "url": "https://github.com/vue-gl/vue-gl/issues"
  },
  "license": "MIT",
  "main": "dist/vue-gl.js",
  "types": "src/index.d.ts",
  "module": "dist/vue-gl.esm.js",
  "files": [
    "/dist/",
    "/src/"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.vue",
    "lint:md": "markdownlint \"**/*.md\" -c markdownlint.config.yml --ignore node_modules --ignore docs/components --ignore _site --ignore docs/examples/*/",
    "test": "jest",
    "start": "rollup -c && bundle exec jekyll serve --source docs",
    "prepare": "rollup -c"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-vue": "^7.0.0",
    "fast-glob": "^3.1.1",
    "jest": "^26.0.0",
    "markdownlint-cli": "^0.26.0",
    "nise": "^4.0.2",
    "pascalcase": "^1.0.0",
    "postcss": "^8.0.3",
    "rollup": "^2.26.6",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-copy": "^3.0.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.1",
    "rollup-plugin-vue": "^5.1.4",
    "rollup-plugin-vuedoc": "^0.4.0",
    "three": "^0.125.2",
    "vue": "^2.2.0",
    "vue-runtime-helpers": "^1.1.2",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "three": "^0.111.0",
    "vue": "^2.2.0"
  }
