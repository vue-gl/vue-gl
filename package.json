{
  "name": "vue-gl",
  "version": "2.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-gl.umd.cjs",
  "module": "./dist/vue-gl.js",
  "exports": {
    ".": {
      "import": "./dist/vue-gl.js",
      "require": "./dist/vue-gl.umd.cjs"
    }
  },
  "description": "Vue.js components rendering 3D graphics reactively via three.js",
  "keywords": [
    "Vue",
    "Vuejs",
    "WebGL",
    "three",
    "threejs",
    "3D",
    "Graphics",
    "Canvas"
  ],
  "homepage": "https://vue-gl.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-gl/vue-gl.git"
  },
  "bugs": {
    "url": "https://github.com/vue-gl/vue-gl/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc && vite build"
  },
  "peerDependencies": {
    "three": "^0.162.0",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@types/three": "^0.161.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vue": "^3.4.21",
    "vue-tsc": "^1.8.27"
  }
}
